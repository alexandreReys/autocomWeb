{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "plugin:vue/base",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["vue"],
  "rules": {
    "space-before-function-paren": "error"
  }
}
